{
  "name": "internetrix/silverstripe-graphql-updatemany",
  "description": "Adds graphql support for an updateMany scaffolder",
  "type": "silverstripe-vendormodule",
  "homepage": "https://gitlab.internetrix.net/silverstripe-modules/graphql-manymanyjoin",
  "keywords": [
    "silverstripe",
    "module",
    "graphql",
    "update",
    "updateMany",
    "scaffolders"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Guy Watson",
      "email": "guy.watson@internetrix.com.au"
    }
  ],
  "support": {
    "issues": "https://gitlab.internetrix.net/silverstripe-modules/graphql-updatemany/issues"
  },
  "require": {
    "silverstripe/vendor-plugin": "^2",
    "silverstripe/framework": "^5",
    "silverstripe/graphql": "^4.3"
  },
  "require-dev": {
    "phpunit/phpunit": "10.2.6",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Internetrix\\GraphQLUpdateMany\\": "src/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true
    }
  }
}
